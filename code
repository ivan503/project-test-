#include <iostream>

using namespace std;

int main()
{
    cout << "Enter number (10...99)" << endl;
    int a=0;
    cin >> a;

    cout << "Edinici chisla A" << " = " << a%10 << endl;
    cout << "Desyatki ot A" << " = " << a/10 << endl;
    char c = a%10;
    char d = a/10;
    cout << "Sum of cifr: " << c + d << endl;



    return 0;
}
